<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ProgrammeFideliteController">
    <children>
        <VBox spacing="10" alignment="CENTER">


            <Label text="Nom du Programme"/>
            <TextField fx:id="nomProgrammeField"/>
            <Label text="Points"/>
            <TextField fx:id="pointsField"/>
            <Button text="Ajouter Programme" onAction="#handleAddProgramme"/>
        </VBox>
    </children>
</AnchorPane>

