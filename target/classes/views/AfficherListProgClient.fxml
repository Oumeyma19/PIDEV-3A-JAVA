<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.cell.PropertyValueFactory?>

<AnchorPane prefHeight="466.0" prefWidth="548.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AfficherListProgrammeClient">
    <TableView fx:id="programmeTable" layoutX="14.0" layoutY="50.0" prefHeight="400.0" prefWidth="520.0">
        <columns>
            <TableColumn fx:id="colId" prefWidth="100.0" text="ID" />
            <TableColumn fx:id="colNom" prefWidth="200.0" text="Nom" />
            <TableColumn fx:id="colPoints" prefWidth="100.0" text="Points" />
            <TableColumn fx:id="colPhoto" prefWidth="120.0" text="Photo" />
            <TableColumn fx:id="colActions" prefWidth="180.0" text="Actions" />
        </columns>
    </TableView>
    <Button fx:id="addProgrammeButton" layoutX="434.0" layoutY="14.0" mnemonicParsing="false" text="Ajouter un programme" />
    <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Retour" />
</AnchorPane>